<link rel="stylesheet" href="/css/sign-up.css">

<div id="sign-out" >
    Welcome to <%=title%> Page
    <form action="/users/create" method="post">
        <input type="text" name="name" placeholder="Enter your name" id="">
        <input type="email" name="email" placeholder="Enter your email" id="">
        <input type="password" name="password" placeholder="Enter your password" id="">
        <input type="password" name="confirm_password" placeholder="Enter password again">
        <input type="submit" value="Sign Up">
    </form>

    <p>
        <a href="/users/sign-in">Go to Sign In Page</a>
    </p>



    <p>
        <a href="/users/forgot-pass">Forgot Password?</a>
    </p>

    <p>
        <a href="/users/auth/google">Google Sign In/Sign Up</a>
    </p>

</div>